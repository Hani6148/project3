    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "express-fileupload": "^1.1.6-alpha.5",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "mongoose": "^5.3.16",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-file-upload": "0.0.4",
    "react-moment": "^0.9.2",
